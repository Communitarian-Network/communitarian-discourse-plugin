<div class="resolution-list-item">
  <div class="resolution-list-item__header">
    <span>Weekly Poll on {{formattedActionPeriod}}</span>
    <span>{{openedPoll.voters}} votes</span>
  </div>
  <p class="resolution-list-item__name">
    {{~topic-link resolution class="raw-link raw-topic-link"}}
  </p>
  <ul class="resolution-list-item__options-list">
    {{#each openedPoll.options as |poll_option|}}
    <li class="resolution-list-item__option">
      <label for="votes" class="resolution-list-item__option-name">
        {{poll_option.html}}
      </label>
      {{!-- <div class="resolution-list-item__option-value">
        {{poll_option.votes}}
      </div> --}}
      <input class="resolution-list-item__option-value" type="range" id="votes"
             name="votes" min="0" max="5" value={{poll_option.votes}}>
    </li>
    {{/each}}
  </ul>
</div>
